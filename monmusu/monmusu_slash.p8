function menu_init ()
	opt=0
end

function menu_update ()
	if (btnp(2)) opt=opt-1
	if (btnp(3)) opt=opt+1
	opt=min(max(opt, 0), 2)

	if btnp(4) then
		if (opt==0) scr_manager.to_game()
		if (opt==1) scr_manager.to_scoreboard()
		if (opt==2) scr_manager.to_instructions()
	end
end

function menu_draw ()
	pal()
	cls()
	-- line(63,0, 63,127, 7)
	draw_text("start", 54, 94, 7, is_lighted(0))
	draw_text("scores", 52, 106, 7, is_lighted(1))
	draw_text("how to play", 42, 118, 7, is_lighted(2))

	-- background
	spr(214, 23,23+osc(1, 100), 10,3)
	spr(0+2*animate_seq(0.3,2), 32,63, 2,2)
	spr(36+2*animate_seq(0.2,3), 78,63, 2,2, true)
	line(27,79, 99,79, 8)
	line(29,80, 97,80, 8)
	line(33,82, 93,82, 8)
end

function is_lighted (id)
	return opt==id and 8 or 0
end

