--	"CLASSES"
--------------------------------------------------------------------------------------------------------------

enemies={
	_arr={},

	new=function (self,typ)
		add(self._arr, {
			x=100,y=79,spd=1.5*min(max(1,(time()-gm_state.start)/8),3),w=8,h=8,
			type=typ or flr(rnd(3))
		})
	end,

	update=function (self)
		for enemy in all(self._arr) do

			enemy.x=enemy.x-enemy.spd
			if (enemy.x <= -8) del(self._arr, enemy)
		end
		if (#self._arr <= 0 and gm_state.can_spawn) self:new()
	end,

	check_colls=function (self,x,y,w,h)
		local hits=0
		for enm in all(self._arr) do
			if	check_bcoll(
					enm.x,enm.y,enm.w,enm.h,
					x,y,w,h)
			then
				if p.state == "attack"
					and p.stance==enm.type%3
				then
					del(self._arr,enm)
					gm_state.kills=gm_state.kills+1
					moon.size=moon.size*1.5
					sfx(0)
					cam:shake(3, 0.5)
				end
				hits=hits+1
			end
		end
		return hits
	end,

	loop_through=function (self, iterator)
		for enemy in all(self._arr) do
			iterator(enemy)
		end
	end,

	del=function (self, enemy)
		del(self._arr, enemy)
	end,

	clear=function (self)
		self._arr={}
	end
}

terrain={
	x=12,y=95,spd=0.035,

	new=function (self)
		self.last_update=0
		self.tiles={}
		for i=0,12 do
			add(self.tiles, rnd(6))
		end
	end,

	update=function (self)
		if (time()-self.last_update<=self.spd) return
		for i=1, #self.tiles do
			if i!=#self.tiles then self.tiles[i]=self.tiles[i+1]
			else self.tiles[i]=rnd(6) end
		end
		self.last_update=time()
	end 
}

cam={
	x=0,y=0,
	s_start=00,
	s_intensity=0,
	s_limit=0,
	shake=function (self, intensity, limit)
		self.s_start=time()
		self.s_intensity=intensity
		self.s_limit=limit
	end,
	update=function (self)
		if time()-self.s_start < self.s_limit then
			self.x=rnd(self.s_intensity)
			self.y=rnd(self.s_intensity)
		else
			self.x = 0
			self.y = 0
		end
	end
}

