function gm_draw ()
	if gm_state.killed then
		pal(8,7)
		pal(0,8)
	else
		pal()
	end
	
	cls(_t(gm_state.killed, 8, 0))
	camera(cam.x,cam.y)
	
	draw_mountains()
	draw_sky()
	draw_terrain()
	draw_player()
	draw_enemies()

	camera()
	draw_hud()
end

function draw_hud()
	if not gm_state.killed then
		draw_stance()
		draw_count(106, 5)
	else
		draw_count(58, 108+osc(1, 100), 7)
		
		print("‚ùé", 85, 112+osc(1, 100, 30), 7)
		print("return", 77, 118+osc(1, 100, 30), 7)

		print("üÖæÔ∏è", 35, 112+osc(1, 100, 60), 7)
		print("replay", 27, 118+osc(1, 100, 60), 7)
	end
end

function draw_count (x,y,c)
	local dec=count_algarisms(gm_state.kills)
	
	print(gm_state.kills,
		x-dec*4,y, c or 8)
	
	-- part
	pal(8, c or 8)
	spr(176, x+4,y-1, 1,1)
	pal()
end

function draw_stance ()
	spr(96+2*p.stance, 6,110, 2,2)
	
	local function drw_indicator (n,x,y)
		if (p.stance!=n-177) pal(8,1)
		spr(n,x,y,1,1)
		pal()
	end
	
	local x=22
	local y=107
	drw_indicator(177,x,		y)
	drw_indicator(178,x+2,	y+7)
	drw_indicator(179,x,		y+13)
end

function draw_enemies ()
	for enemy in all(enemies._arr) do
		spr(64+4*enemy.type+2*(flr(time()*10)%2)*_t(gm_state.killed, 0, 1), enemy.x, enemy.y, 2,2, true)
		pal(8, 1)
		spr(177+enemy.type, enemy.x+2, enemy.y+20, 1,1, true)
		pal()
	end
end

function draw_player ()
	local frames=p.anim[p.state]
	spr(_t(gm_state.killed, 4, frames[p.anim_count]),p.x,p.y,2,2)
	
	-- draw sword on attack and slash effect
	if p.state=="attack" and not gm_state.killed 
	then
		if p.anim[p.state][p.anim_count]==34 then
			line(p.x-1,p.y+7, p.x-4,p.y+10, 8)
			line(p.x-1,p.y+8, p.x-4,p.y+11, 8)
			line(p.x-1,p.y+9, p.x-3,p.y+11, 8)
		end

		pal(7, 8)
		sspr(48+16*min(p.anim_count, 4), 64, 16,16, p.x+3, p.y-6, 26,24)
		pal(7, 7)
		sspr(48+16*min(p.anim_count, 4), 64, 16,16, p.x+5, p.y-7, 24,24)
	end
end

function draw_terrain ()
	local w=#terrain.tiles
	local x=terrain.x
	local y=terrain.y

	line(x-1,y, x+w*8,y, 8)
	local offset=0
	for tile in all(terrain.tiles) do
		sspr(0+tile*4,120,
			8,4,
			x+offset*8,y)
		offset=offset+1
	end
	trifill(x-2,y,5,0)
	trifill(x+w*8+1,y,5,0)
end

function draw_mountains ()
	trifill(35, 53, 20)
	trifill(56, 63, 10)
	trifill(76, 58, 15)
	trifill(92, 55, 18)
	rectfill(15,74, 110,76, 2)
	draw_bmask()
	draw_cloud(80+osc(4,9,85),68,40)
	draw_cloud(86+osc(4,9,85),65,40)
	draw_cloud(26+osc(4,9,85),72,20)
	draw_cloud(13+osc(4,9,45),62,20)
end

function draw_bmask ()
	circ(63,11,64,0)
	for i=0,20 do 
		circ(63,13,64+i,0)
	end
end

function trifill (x,y, s,c)
	for i=0,s do
		line(x-s+i,y+s, x,y+i, c or 2)
	end
	for i=0,s do
		line(x+i,y+s, x,y+s-i, c or 2)
	end
end

function draw_sky ()
	circ(63,31+osc(2,9),moon.size,8)
	circfill(63,31+osc(2,9), moon.size-3, 8)
	draw_cloud(11+osc(4,9,85),19+osc(1,4.5),40)
	draw_cloud(17+osc(4,9,90),22+osc(1,4.5,45),40)
	draw_cloud(70+osc(4,9,175),33+osc(1,4.5,90),40)
	draw_cloud(64+osc(4,9,180),36+osc(1,4.5,135),40)
	draw_cloud(16+osc(4,9,45),45+osc(1,4.5,180),40)
end

-- x,y, width,max_size, color
function draw_cloud (x,y,w,h,clr)
	for lx=x,x+w do
		local fact=_t(lx<=x+w/2,
			(lx-x)/w,
			(w-(lx-x))/w
		)
		circfill(lx,y,(h or 3)*fact+2,clr or 0)
	end
end

-- x,y, width
function draw_gnd (x,y,w)
	for i=0,w do	
		sspr(0+flr(rnd(5))*8,120+flr(rnd())*4,
			8,4, (x or 0)+i*8,y)
	end
end

