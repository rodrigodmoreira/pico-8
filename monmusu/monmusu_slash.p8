--------------------------------------------------------------------------------------------------------------
--	UPDATE / IN GAME
--------------------------------------------------------------------------------------------------------------

function gm_update ()
	-- end screen update
	if gm_state.killed then
		if btnp(4) then
			save_to_scoreboard(gm_state.kills)
			scr_manager.to_game()
			return
		elseif btnp(5) then
			save_to_scoreboard(gm_state.kills)
			scr_manager.to_menu()
			return
		end
	end

	-- game update
	if not gm_state.killed then	
		-- handle input
		if (btnp(2)) p.stance=0
		if (btnp(1)) p.stance=1
		if (btnp(3)) p.stance=2
		if btnp(4) and time()-p.last_atk > 0.4 then
			p.state="attack"
			p.anim_count=0
			sfx(0)
		end

		enemies:loop_through(function (enemy)
			-- check if dead
			if	check_bcoll(
					enemy.x,enemy.y,enemy.w,enemy.h,
					p.x-8,p.y,p.w,p.h)
			then
				gm_state.killed=true
			end

			-- check if attacked
			if p.state=="attack" then
				if	check_bcoll(
						enemy.x,enemy.y,enemy.w,enemy.h,
						p.x+8,p.y,p.w,p.h)
					and p.stance==enemy.type%3
					and p.anim_count>2
				then
					enemies:del(enemy)
					gm_state.kills=gm_state.kills+1
					moon.size=moon.size*1.5
					cam:shake(3, 0.5)
				end
			end
		end)
	end
	
	-- animation updates
	update_player_anim()
	moon.size=lerp(moon.size,22,0.25)

	if not gm_state.killed then
		enemies:update()
		terrain:update()
		
		if time() - sound.steps > 0.12 then
			sfx(1)
			sound.steps=time()
		end
	end

	cam:update()
end

function update_player_anim ()
	if (time()-p.last_frame>=p.anim_spd) then
		p.last_frame=time()
		if p.anim_count+1>#p.anim[p.state] then
			if p.state == "attack" then
				p.state="runn"
				p.anim_count=1
			else
				-- loop anim
				p.anim_count=1
			end
		else
			p.anim_count=p.anim_count+1
		end
	end
end

