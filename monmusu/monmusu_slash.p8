function gm_init ()
	music(2)
	gm_state={
		can_spawn=true,
		killed=false,
		kills=0,
		start=time()
	}
	
	p={
		x=15,y=79,
		w=16,h=16,
		stance=0,
		
		state="runn",
		last_frame=0, --last frame updt
		anim_spd=0.05, --sec btw frames
		anim_count=1,
		anim={
			idle={0,2},
			runn={4,6,8,10},
			attack={12,14,32,34,34}
		},
		
		last_atk=0
	}

	-- last time played
	sound={
		steps=0
	}

	moon={
		size=22
	}
	
	terrain:new()
end

