--	UTILS
--------------------------------------------------------------------------------------------------------------

function _t (exp, a,b)
	return exp and a or b
end

function osc(max_value, spd, offset)
	return max_value*sin(spd*(time()+(offset or 0)) * 3.14/180)
end

function check_bcoll(
	x1,y1,w1,h1,
	x2,y2,w2,h2
)
	return	x1+w1 >= x2 -- +x_offset
				and x1   	<= x2+w2
				and y1+h1 >= y2
				and y1 			<= y2+h2
end

function count_algarisms (n)
	local dec=0
	local _ct=abs(n)
	while _ct>0 do
		_ct=flr(_ct/10)
		dec=dec+1
	end
	return max(dec,1)-1
	
end

function lerp (a,b, t)
	return a+(b-a)*t
end

function animate_seq (delay, nmb_frames)
	return (flr(time()/delay))%nmb_frames
end

function draw_text (str, x,y, clr, rclr)
	rectfill(x-2,y-2, x+#str*4, y+6, rclr)
	rectfill(x-3,y-1, x+#str*4+1, y+5, rclr)
	print(str, x,y, clr)
end

function load_scoreboard ()
	local arr={}
	for i=0, 9 do
		add(arr, dget(i))
	end
	return arr
end

function save_to_scoreboard (score)
	local saved_scores = load_scoreboard()
	for i=1, #saved_scores do
		local svd_scr = saved_scores[i]
		-- found position to insert and move everyone foward
		if score > svd_scr then
			dset(i-1, score)
			for j=i, #saved_scores-1 do
				dset(j, saved_scores[j])
			end
			return
		end
	end
end

scr_manager={
	to_menu=function()
		menu_init()
		_update=menu_update
		_draw=menu_draw
	end,
	to_game=function()
		gm_init()
		enemies:clear()
		_update=gm_update
		_draw=gm_draw
	end,
	to_instructions=function()
		instructions_init()
		_update=instructions_update
		_draw=instructions_draw
	end,
	to_scoreboard=function()
		scoreboard_init()
		_update=scoreboard_update
		_draw=scoreboard_draw
	end
}

